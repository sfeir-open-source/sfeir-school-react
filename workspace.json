{
  "version": 2,
  "projects": {
    "docs": {
      "projectType": "application",
      "root": "apps/docs",
      "sourceRoot": "apps/docs/src",
      "targets": {
        "sass": {
          "executor": "@nrwl/workspace:run-script",
          "options": {
            "script": "sass"
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-script",
          "options": {
            "script": "serve"
          }
        }
      },
      "tags": []
    },
    "solution": {
      "root": "apps/solution",
      "sourceRoot": "apps/solution/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:webpack",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/solution",
            "index": "apps/solution/src/index.html",
            "baseHref": "/",
            "main": "apps/solution/src/main.tsx",
            "polyfills": "apps/solution/src/polyfills.ts",
            "tsConfig": "apps/solution/tsconfig.app.json",
            "assets": [
              "apps/solution/src/favicon.ico",
              "apps/solution/src/assets"
            ],
            "styles": [
              "apps/solution/src/styles.scss"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/solution/src/environments/environment.ts",
                  "with": "apps/solution/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "solution:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "solution:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/solution/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/solution"
          ],
          "options": {
            "jestConfig": "apps/solution/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "solution-e2e": {
      "root": "apps/solution-e2e",
      "sourceRoot": "apps/solution-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/solution-e2e/cypress.json",
            "devServerTarget": "solution:serve",
            "tsConfig": "apps/solution-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "solution:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/solution-e2e/**/*.{js,ts}"
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": [
        "solution"
      ]
    }
  }
}